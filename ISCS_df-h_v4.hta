                                            var diffYelGre = 95/100; //percent gap between yellow and green. means how far green will turn yellow
                                            var colRed, colGre = 0, colBlu = 0;
                                            if (!(isNaN(redHigh))) {
                                                /*if (percentValue >= redHigh) {
                                                    colRed = 255; colGre = 0;
                                                } else */
                                                if (percentValue >= (redHigh * diffYelGre) && (percentValue < redHigh) ){
                                                    aboveYelValue = percentValue - (redHigh * diffYelGre);
                                                    aboveYelUnit = 255 / (redHigh - (redHigh * diffYelGre));
                                                    colGre = 255 - (aboveYelValue * aboveYelUnit);
                                                    colRed = 255;
                                                } else if ((percentValue > 1) && (percentValue < (redHigh * diffYelGre))){
                                                    colGre = 255;
                                                    colRed = percentValue * (255/(redHigh * diffYelGre));
                                                } else if (percentValue <= 1){
                                                    colGre = 255;
                                                    colRed = 0;
                                                }
                                            }
                                            if ((parseInt(percentValue) !== NaN) && (percentValue < redHigh)){
                                                cell.setAttribute("bgcolor", "rgb(" + parseInt(colRed) + "," + parseInt(colGre) + "," + colBlu + ")")
                                                cell.style.setAttribute('width', '125px');
                                            }
                                        } else {
                                            cell.setAttribute("bgcolor", "transparent")
                                            cell.style.setAttribute('width', '125px');
                                        }
                                    }




                                //cellText = window.document.createTextNode(lineArrayOfObjects[index][element]);
                                var cellPercentValue = (isNaN(percentValue))?percentValue:percentValue + '%';
                                cellText = window.document.createTextNode(cellPercentValue);
                                //cell.setAttribute("id", lineArrayOfObjects[index]['newServerName'] + "_" + element + "_" + lineArrayOfObjects[index][element]);
                                cell.setAttribute("id", lineArrayOfObjects[index]['newServerName'] + "_" + element + "_" + percentValue + "_" + redHigh);
                                //id=serverName_/sliceName_percentage%
                                cell.appendChild(cellText);
                                //cell.style.setAttribute('text-align', 'center');
                                row.appendChild(cell);
                            });
                            tblBody.appendChild(row);

                        });


                    // put the <tbody> in the <table>
                    tbl.appendChild(tblBody);
                    // appends <table> into <body>
                    body.appendChild(tbl);
                    // sets the border attribute of tbl to 2;
                    tbl.setAttribute("border", "2");
                self.resizeTo('1','1');
                sleepingVB();
                self.resizeTo(screen.width,screen.height-50);
                self.moveTo(5,5);
                body.style.setAttribute("height","200px")

            fileFolderHelper.DeleteFile('.\\output_' + newDateFileName);
            fileFolderHelper.DeleteFile('.\\input_' + newDateFileName);
            //fileFolderHelper.DeleteFolder('.\\input');
            //fileFolderHelper.DeleteFolder('.\\output');
            }
