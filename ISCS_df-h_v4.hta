    function tickSelector(){
        var tickSelector_TableBody = window.document.getElementById('resultTable');
        var tickSelector_TagList = tickSelector_TableBody.getElementsByTagName('TD');
        var hostnameInID = [], childIDColor=[], serverRED=[], serverGreen=[], serverOther=[];
        for(var i = 0; i < tickSelector_TagList.length; i ++){
            if(tickSelector_TagList[i].getAttribute('id') != null) {
                hostnameInID.push(tickSelector_TagList[i].getAttribute('id'));
                childIDColor.push(tickSelector_TagList[i].getAttribute('bgcolor'));
            }
        }
        //("id", lineArrayOfObjects[index]['newServerName'] + "_" + element + "_" + percentValue + "_" + redHigh);
        //cell.setAttribute("bgcolor", "rgb(255,0,0)");
        for (var i =0; i < childIDColor.length; i ++) {
            if(childIDColor[i].indexOf('rgb(255,0,0)') > -1){
                serverRED.push(hostnameInID[i].split('_')[0].trim());
            } else {
                serverOther.push(hostnameInID[i].split('_')[0].trim());
            }
        }

        serverRED.sort(); //sort the servername, maybe faster duplicate filter
        serverOther.sort();
        var serverREDFiltered = serverRED.filter(function(elem, index, self) { //removes duplicates of Red Server
            return index == self.indexOf(elem);
        })
        var serverOtherFiltered = serverOther.filter(function(elem, index, self) { //removes duplicates of Red Server
            return index == self.indexOf(elem);
        })

        for(var i=0; i < serverOtherFiltered.length; i++){
            serverREDFiltered.forEach(function(elem){
                if(serverOtherFiltered[i] == elem){
                    serverOtherFiltered.splice(i,1);
                    i--;
                }
            });
        }

        /*
        <tr class="eachRows" id='dt02-sms-01' align=center>
        <td class="tdA4" >DT02/SMS01</td>
        <td class="tdA4" ></td>
        <td class="tdA4" ></td>
        <td class="tdA4" ></td>
        <td class="tdA4" ></td>
        <td class="tdA4 thisOK" style="font-size:35px;color:transparent;"><label  class="labelA4" onclick="javascript:clickToggle(this,'OK');">&#10003;</label></td>
        <td class="tdA4 thisNOK" style="font-size:35px;color:transparent;"><label  class="labelA4" onclick="javascript:clickToggle(this,'NOK');">&#10003;</label></td>
        <td class="tdA4" ><textarea style='resize:none;' class="textarea"></textarea></textarea></td>
        </tr>
        */
        var tickSelector_A4TableBody = window.document.getElementById('a4First');
        var tickSelector_TrRow=tickSelector_A4TableBody.getElementsByTagName('TR');
        for(var j=0; j < tickSelector_TrRow.length; j++){
            if ((tickSelector_TrRow[j].getAttribute('id') !== null) && (tickSelector_TrRow[j].getAttribute('id').indexOf('rowHead') == -1))  {
                var cellTdA4 = tickSelector_TrRow[j].getElementsByTagName('TD');
                cellTdA4[5].style.color = 'transparent'; //OK
                cellTdA4[6].style.color = 'transparent'; //NOK
            }
        }
        for(var j=0; j < tickSelector_TrRow.length; j++){//at A4 Page go through every row
        //if ((tickSelector_TrRow[j].getAttribute('id') == null) || (tickSelector_TrRow[j].getAttribute('id').indexOf('rowHead') > -1)) {//skip irrelevant ID.
        //    continue;
        //}
            if (tickSelector_TrRow[j].getAttribute('id') !== null) {
                for (var k=0; k < serverREDFiltered.length; k++){
                    if (tickSelector_TrRow[j].getAttribute('id').indexOf(serverREDFiltered[k]) > -1){ //NOK result
                        var cellTdA4 = tickSelector_TrRow[j].getElementsByTagName('TD');
                        cellTdA4[5].style.color = 'transparent'; //OK
                        cellTdA4[6].style.color = 'black'; //NOK
                        break;
                    }
                }
                for (var k=0; k < serverOtherFiltered.length; k++){
                    if (tickSelector_TrRow[j].getAttribute('id').indexOf(serverOtherFiltered[k]) > -1){ //NOK result
                        var cellTdA4 = tickSelector_TrRow[j].getElementsByTagName('TD');
                        cellTdA4[5].style.color = 'black'; //OK
                        cellTdA4[6].style.color = 'transparent'; //NOK
                        break;
                    }
                }
            }

        }
    }

